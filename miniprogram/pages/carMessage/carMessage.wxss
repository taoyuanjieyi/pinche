page {
  background: #eaeaea;
}

.carMessage {
  background: #fff;
}

.carMessage-list {
  width: 100%;
  padding: 14rpx 5%;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eaeaea;
}

.carMessage-list-img {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  border: 1rpx solid #eaeaea;
}

.carMessage-list-title {
  font-size: 32rpx;
  color: #379ff8;
}

.carMessage-list-message {
  font-size: 28rpx;
  margin-top: 10rpx;
}

.carMessage-arrow::after {
  margin-left: 30rpx;
  right: 0;
  content: " ";
  display: inline-block;
  height: 26rpx;
  width: 16rpx;
  position: relative;
  border-radius: 0;
  background: url("data:image/png;base64,") no-repeat center center;
  background-size: 16rpx;
}

.container {
  background: #fff;
  overflow: hidden;
}

.commonStroke-list-edit {
  min-width: 26rpx;
  min-height: 26rpx;
  width: 26rpx;
  height: 26rpx;
  margin-left: 20rpx;
  padding: 10rpx;
}

.del {
  background: #ff4500;
}
