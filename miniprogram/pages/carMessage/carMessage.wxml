<view class='carMessage-top'>
  <view bindtap='goBack'>
    <!--返回上一级-->
  </view>
  <view class='carMessage-top-add' bindtap='goAdd'>+</view>
</view>
<view class='carMessage'>
  <label wx:for="{{items}}" wx:key='' wx:for-index="aindex">
    <view class='carMessage-list'>
      <image class='carMessage-list-img' mode='aspectFill' src='https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS0osamzWoVrqQTb6QR97j0aMxfpY-sDE1WC1cE8_cLhyCg0m90'></image>
      <view>
        {{item.carNumber}} {{item.color}} {{item.brand}}
        <!-- <view class='carMessage-list-title'></view> -->
        <!-- <view class='carMessage-list-message'>车辆信息</view> -->
      </view>
    </view>
  </label>
  <!-- <view class='carMessage-arrow'></view> -->
</view>
<view class='carMessage-no' hidden="{{noDataHide}}">暂无车辆信息</view>